{
  "name": "gh-packages-test",
  "version": "1.0.1",
  "description": "Just a simple test project to test GitHub Packages",
  "main": "index.js",
  "scripts": {
    "serve": "npm run build && npm run start",
    "start": "node dist/app.js",
    "prebuild": "node -p \"JSON.stringify({'version': require('./package.json').version})\" > .appversion.json",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postpack": "node -p \"require('./package.json').name.concat('-').concat(require('./package.json').version).concat('.tgz')\" > .artifact-name"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voronin-de/gh-packages-test.git"
  },
